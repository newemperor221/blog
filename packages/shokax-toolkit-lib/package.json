{
  "name": "shokax-toolkit-lib",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.0",
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./posts/*": {
      "import": "./src/posts/*.ts",
      "types": "./src/posts/*.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:e2e": "echo 'No test specified' && exit 0",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@antfu/eslint-config": "latest",
    "@ctrl/tinycolor": "^4.2.0",
    "eslint": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest"
  },
  "devDependencies": {
    "typescript": "latest",
    "vite": "latest"
  }
}
